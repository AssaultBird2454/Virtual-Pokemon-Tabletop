<UserControl x:Class="AssaultBird2454.VPTU.Client.Class.Controls.Battler.Character_Tile"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:AssaultBird2454.VPTU.Client.Class.Controls.Battler"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300" Height="42" Width="30">
    <Grid>
        <Border BorderThickness="1" HorizontalAlignment="Left" VerticalAlignment="Top" Width="30" Height="35">
            <Border.Background>
                <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/004.png" Stretch="Uniform"/>
            </Border.Background>
        </Border>
        <ProgressBar HorizontalAlignment="Left" Height="7" Margin="0,35,0,0" VerticalAlignment="Top" Width="30" Maximum="50" Background="{x:Null}" FlowDirection="RightToLeft" Foreground="Red" Value="5" BorderBrush="{x:Null}"/>
        <ProgressBar HorizontalAlignment="Left" Height="7" Margin="0,35,0,0" VerticalAlignment="Top" Width="30" Value="20" Maximum="50" Background="{x:Null}" BorderBrush="{x:Null}"/>
        <Border BorderThickness="1" HorizontalAlignment="Left" Height="5" VerticalAlignment="Top" Width="5" Background="Red"/>

    </Grid>
</UserControl>
