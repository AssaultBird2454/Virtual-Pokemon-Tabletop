<UserControl x:Class="AssaultBird2454.VPTU.Client.UI.Entities.EntitiesList"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:AssaultBird2454.VPTU.Client.UI.Entities"
             mc:Ignorable="d" d:DesignWidth="400" d:DesignHeight="400">
    <Grid>
        <TreeView x:Name="Tree" Margin="0,27,0,0" />
        <ToolBar x:Name="ToolBar" VerticalAlignment="Top">
            <Button x:Name="ToolBar_Reload" Width="22" Height="22" Margin="0" Click="ToolBar_Reload_Click">
                <Button.Background>
                    <ImageBrush ImageSource="pack://siteoforigin:,,,/Resources/Reload.png" Stretch="Uniform" />
                </Button.Background>
            </Button>
            <Separator />
            <Separator />
        </ToolBar>
    </Grid>
</UserControl>