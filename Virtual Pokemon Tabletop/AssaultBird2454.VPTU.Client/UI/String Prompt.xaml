<Window x:Class="AssaultBird2454.VPTU.Client.UI.String_Prompt"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssaultBird2454.VPTU.Client.UI"
        mc:Ignorable="d"
        Title="String_Prompt" Height="100" Width="300" ResizeMode="NoResize">
    <Grid>
        <TextBox x:Name="Value" Height="23" Margin="10,10,10,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" />
        <Button x:Name="Sumbit_Button" Content="Submit" HorizontalAlignment="Left" Margin="10,0,0,10"
                VerticalAlignment="Bottom" Width="75" Click="Sumbit_Button_Click" />
        <Button x:Name="Cancel_Button" Content="Cancel" HorizontalAlignment="Left" Margin="90,0,0,10"
                VerticalAlignment="Bottom" Width="75" Click="Cancel_Button_Click" />

    </Grid>
</Window>