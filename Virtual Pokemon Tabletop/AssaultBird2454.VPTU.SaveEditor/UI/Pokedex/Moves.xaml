<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AssaultBird2454.VPTU.SaveEditor.UI.Pokedex"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit" x:Class="AssaultBird2454.VPTU.SaveEditor.UI.Pokedex.Moves"
        mc:Ignorable="d"
        Title="Moves" Height="802" Width="614">
    <Grid>
        <GroupBox x:Name="groupBox" Header="GroupBox" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="10,10,0,0" Height="101" Width="383">
            <Grid Margin="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition/>
                </Grid.ColumnDefinitions>
                <TextBox x:Name="Basic_Name" Margin="90,4,0,0" TextWrapping="Wrap" VerticalAlignment="Top" HorizontalAlignment="Left" Width="271"/>
                <Label Content="Move Name" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                <TextBox x:Name="Basic_Desc" HorizontalAlignment="Left" Margin="90,27,0,0" TextWrapping="Wrap" Width="271" Height="45" VerticalAlignment="Top"/>
            </Grid>
        </GroupBox>
        <GroupBox x:Name="groupBox1" Header="Contest Details" HorizontalAlignment="Left" Margin="398,10,0,0" VerticalAlignment="Top" Height="101" Width="206">
            <Grid Margin="0">
                <Label x:Name="label" Content="Type" HorizontalAlignment="Left" VerticalAlignment="Top"/>
                <ComboBox x:Name="Contest_Type" HorizontalAlignment="Left" Margin="43,4,0,0" VerticalAlignment="Top" Width="120"/>
                <Label x:Name="label_Copy" Content="Effect" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="0,27,0,0"/>
                <ComboBox x:Name="Contest_Effect" HorizontalAlignment="Left" Margin="43,31,0,0" VerticalAlignment="Top" Width="120"/>

            </Grid>
        </GroupBox>
        <GroupBox x:Name="groupBox1_Copy2" Header="Battle Details" HorizontalAlignment="Left" Margin="10,116,0,0" VerticalAlignment="Top" Height="449" Width="586">
            <Grid Margin="0">
                <GroupBox x:Name="groupBox1_Copy1" Header="Range Details" Margin="0,0,207,0" Height="254" VerticalAlignment="Top">
                    <Grid Margin="0">
                        <CheckBox x:Name="Range_Enabled" Content="Range" HorizontalAlignment="Left" Margin="10,28,0,0" VerticalAlignment="Top"/>
                        <xctk:DecimalUpDown x:Name="Range_Distance" HorizontalAlignment="Left" Margin="100,26,0,0" VerticalAlignment="Top" Width="58"/>
                        <Label Content="Distance" HorizontalAlignment="Left" Margin="100,0,0,0" VerticalAlignment="Top" Width="58"/>
                        <Label Content="Size" HorizontalAlignment="Left" Margin="163,0,0,0" VerticalAlignment="Top" Width="58"/>
                        <CheckBox x:Name="Line_Enabled" Content="Line" HorizontalAlignment="Left" Margin="10,53,0,0" VerticalAlignment="Top"/>
                        <xctk:DecimalUpDown x:Name="Line_Size" HorizontalAlignment="Left" Margin="163,51,0,0" VerticalAlignment="Top" Width="58"/>
                        <CheckBox x:Name="RangedBlast_Enabled" Content="Ranged Blast" HorizontalAlignment="Left" Margin="10,78,0,0" VerticalAlignment="Top"/>
                        <xctk:DecimalUpDown x:Name="RangedBlast_Distance" HorizontalAlignment="Left" Margin="100,76,0,0" VerticalAlignment="Top" Width="58"/>
                        <xctk:DecimalUpDown x:Name="RangedBlast_Size" HorizontalAlignment="Left" Margin="163,76,0,0" VerticalAlignment="Top" Width="58"/>
                        <CheckBox x:Name="Cone_Enabled" Content="Cone" HorizontalAlignment="Left" Margin="10,103,0,0" VerticalAlignment="Top"/>
                        <xctk:DecimalUpDown x:Name="Cone_Size" HorizontalAlignment="Left" Margin="163,101,0,0" VerticalAlignment="Top" Width="58"/>
                        <CheckBox x:Name="CloseBlast_Enabled" Content="Close Blast" HorizontalAlignment="Left" Margin="10,128,0,0" VerticalAlignment="Top"/>
                        <xctk:DecimalUpDown x:Name="CloseBlast_Size" HorizontalAlignment="Left" Margin="163,126,0,0" VerticalAlignment="Top" Width="58"/>
                        <CheckBox x:Name="Burst_Enabled" Content="Burst" HorizontalAlignment="Left" Margin="10,153,0,0" VerticalAlignment="Top"/>
                        <xctk:DecimalUpDown x:Name="Burst_Size" HorizontalAlignment="Left" Margin="163,151,0,0" VerticalAlignment="Top" Width="58"/>
                        <CheckBox x:Name="Self_Enabled" Content="Self" HorizontalAlignment="Left" Margin="10,207,0,0" VerticalAlignment="Top"/>
                        <CheckBox x:Name="Mele_Enabled" Content="Mele" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="11,186,0,0"/>
                        <CheckBox x:Name="Field_Enabled" Content="Field" HorizontalAlignment="Left" Margin="114,186,0,0" VerticalAlignment="Top"/>
                        <CheckBox x:Name="AnyTarget_Enabled" Content="Any Target" HorizontalAlignment="Left" Margin="114,207,0,0" VerticalAlignment="Top"/>
                        <CheckBox x:Name="CardinallyAdj_Enabled" Content="Cardinally Adjacent" HorizontalAlignment="Left" Margin="217,186,0,0" VerticalAlignment="Top"/>
                        <CheckBox x:Name="AllAdj_Enabled" Content="All Adjacent Foes" HorizontalAlignment="Left" Margin="217,207,0,0" VerticalAlignment="Top"/>
                    </Grid>
                </GroupBox>
                <GroupBox x:Name="groupBox1_Copy" Header="Move Keywords" Margin="0,254,207,0">
                    <Grid Margin="0">
                        <WrapPanel ScrollViewer.CanContentScroll="True">
                            <CheckBox x:Name="Keyword_Aura" Content="Aura"/>
                            <CheckBox x:Name="Keyword_Berry" Content="Berry"/>
                            <CheckBox x:Name="Keyword_Blessing" Content="Blessing"/>
                            <CheckBox x:Name="Keyword_Coat" Content="Coat"/>
                            <CheckBox x:Name="Keyword_Dash" Content="Dash"/>
                            <CheckBox x:Name="Keyword_DubleStrike" Content="Duble Strike"/>
                            <CheckBox x:Name="Keyword_Environ" Content="Environ"/>
                            <CheckBox x:Name="Keyword_Execute" Content="Execute"/>
                            <CheckBox x:Name="Keyword_Fling" Content="Fling"/>
                            <CheckBox x:Name="Keyword_Friendly" Content="Friendly"/>
                            <CheckBox x:Name="Keyword_Illusion" Content="Illusion"/>
                            <CheckBox x:Name="Keyword_FiveStrike" Content="FiveStrike"/>
                            <CheckBox x:Name="Keyword_Interupt" Content="Interupt"/>
                            <CheckBox x:Name="Keyword_Pass" Content="Pass"/>
                            <CheckBox x:Name="Keyword_GroundSource" Content="Ground Source"/>
                            <CheckBox x:Name="Keyword_Hazard" Content="Hazard"/>
                            <CheckBox x:Name="Keyword_Pledge" Content="Pledge"/>
                            <CheckBox x:Name="Keyword_Shield" Content="Shield"/>
                            <CheckBox x:Name="Keyword_Smite" Content="Smite"/>
                            <CheckBox x:Name="Keyword_Social" Content="Social"/>
                            <CheckBox x:Name="Keyword_Powder" Content="Powder"/>
                            <CheckBox x:Name="Keyword_Priority" Content="Priority"/>
                            <CheckBox x:Name="Keyword_Sonic" Content="Sonic"/>
                            <CheckBox x:Name="Keyword_SpiritSurge" Content="Spirit Surge"/>
                            <CheckBox x:Name="Keyword_Trigger" Content="Trigger"/>
                            <CheckBox x:Name="Keyword_Vortex" Content="Vortex"/>
                            <CheckBox x:Name="Keyword_Push" Content="Push"/>
                            <CheckBox x:Name="Keyword_Weather" Content="Weather"/>
                            <CheckBox x:Name="Keyword_Reaction" Content="Reaction"/>
                            <CheckBox x:Name="Keyword_Recoil" Content="Recoil"/>
                            <CheckBox x:Name="Keyword_Setup" Content="Setup"/>
                            <CheckBox x:Name="Keyword_Hail" Content="Hail"/>
                            <CheckBox x:Name="Keyword_Rainy" Content="Rainy"/>
                            <CheckBox x:Name="Keyword_Sandstorm" Content="Sandstorm"/>
                            <CheckBox x:Name="Keyword_Sunny" Content="Sunny"/>
                            <CheckBox x:Name="Keyword_WeightClass" Content="Weight Class"/>
                        </WrapPanel>
                    </Grid>
                </GroupBox>
                <Label Content="Move Type" HorizontalAlignment="Right" Margin="0,13,133,0" VerticalAlignment="Top"/>
                <ComboBox x:Name="Battle_Type" Margin="0,15,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="117"/>
                <Label Content="Move Class" Margin="0,40,132,0" VerticalAlignment="Top" HorizontalAlignment="Right"/>
                <ComboBox x:Name="Battle_Class" Margin="0,42,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="117"/>
                <Label Content="Action Type" HorizontalAlignment="Right" Margin="0,173,129,0" VerticalAlignment="Top"/>
                <ComboBox x:Name="Battle_ActionType" Margin="0,175,10,0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="117"/>
                <Label Content="Frequency" Margin="0,200,137,0" VerticalAlignment="Top" HorizontalAlignment="Right"/>
                <ComboBox x:Name="Battle_Frequency" Margin="0,202,10,0" HorizontalAlignment="Right" Width="117" VerticalAlignment="Top"/>
                <xctk:DecimalUpDown x:Name="Battle_UseLimit" HorizontalAlignment="Right" Margin="0,229,10,0" Width="117" Watermark="Pokedex ID" TextAlignment="Center" VerticalAlignment="Top" Value="0" Text="" Minimum="0" Maximum="10000"/>
                <Label Content="Freq Limit" Margin="0,226,139,0" VerticalAlignment="Top" HorizontalAlignment="Right"/>
                <Label Content="DB" Margin="0,251,177,0" VerticalAlignment="Top" HorizontalAlignment="Right"/>
                <xctk:DecimalUpDown x:Name="Battle_DB" HorizontalAlignment="Right" Margin="0,254,10,0" Width="117" Watermark="Pokedex ID" TextAlignment="Center" VerticalAlignment="Top" Value="0" Text="" Minimum="0" Maximum="28"/>
                <Label Content="AC" Margin="0,276,177,0" VerticalAlignment="Top" HorizontalAlignment="Right"/>
                <xctk:DecimalUpDown x:Name="Battle_AC" HorizontalAlignment="Right" Margin="0,279,10,0" Width="117" Watermark="Pokedex ID" TextAlignment="Center" VerticalAlignment="Top" Value="0" Text="" Minimum="0" Maximum="28"/>
            </Grid>
        </GroupBox>
        <GroupBox x:Name="groupBox2" Header="Battle Effects" HorizontalAlignment="Left" Margin="10,570,0,0" VerticalAlignment="Top" Height="190" Width="395">
            <Grid Margin="0">
                <Label x:Name="label1" Content="Feature not implemented...&#xD;&#xA;This depends on the&#xD;&#xA;Battle Effect manager lib" HorizontalContentAlignment="Center" VerticalContentAlignment="Center"/>
            </Grid>
        </GroupBox>
        <Button x:Name="Cancel_Button" Content="Cancel" HorizontalAlignment="Left" Margin="521,570,0,0" VerticalAlignment="Top" Width="75" IsCancel="True" Click="Cancel_Button_Click"/>
        <Button x:Name="Submit_Button" Content="Submit" HorizontalAlignment="Left" Margin="441,570,0,0" VerticalAlignment="Top" Width="75" IsDefault="True" Click="Submit_Button_Click"/>

    </Grid>
</Window>
